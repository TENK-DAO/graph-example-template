{
  "name": "monkegodz",
  "version": "0.0.0",
  "author": "r-srawser",
  "repository": "https://github.com/r-strawser/tenk-subgraph-template.git",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph codegen && graph build",
    "auth": "graph auth --product=hosted-service $THE_GRAPH_KEY",
    "deploy": "graph codegen && graph deploy willemneal/monkegodz",
    "create-local": "graph create --node http://localhost:8020/ willemneal/monkegodz",
    "remove-local": "graph remove --node http://localhost:8020/ willemneal/monkegodz",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 willemneal/monkegodz"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.26.0",
    "@graphprotocol/graph-ts": "^0.24.1"
  }
}
